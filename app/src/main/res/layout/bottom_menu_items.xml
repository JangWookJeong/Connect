<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <LinearLayout
        android:id="@+id/Main_Linear_Four"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <ImageButton
            android:id="@+id/main_bottom_home_button"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="5dp"
            android:layout_marginTop="5dp"
            android:layout_weight="1"
            android:background="#00000000"
            android:src="@drawable/home_bottom_icon_selector"

            />
        <!--262d40-->
        <ImageButton
            android:id="@+id/main_bottom_searchbutton"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="5dp"
            android:layout_marginTop="5dp"
            android:layout_weight="1"
            android:background="#00000000"
            android:src="@drawable/play_bottom_icon_selector"
            android:clickable="true"/>

        <ImageButton
            android:id="@+id/main_bottom_notification_button"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="5dp"
            android:layout_marginTop="5dp"
            android:layout_weight="1"
            android:background="#00000000"
            android:src="@drawable/bookmark_bottom_icon_selector"
            android:clickable="true"/>

        <ImageButton
            android:id="@+id/main_bottom_myPage_button"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="5dp"
            android:layout_marginTop="5dp"
            android:layout_weight="1"
            android:background="#ffffff"
            android:src="@drawable/mypage_bottom_icon_selector"
            android:clickable="true"/>


    </LinearLayout>

</RelativeLayout>